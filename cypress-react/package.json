{
  "name": "cypress-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 3001",
    "dev:wait": "start-test dev 3001",
    "cy:open": "cross-env NODE_ENV=test pnpm run dev:wait \"cypress open\"",
    "cy:run": "cross-env NODE_ENV=test pnpm run dev:wait \"cypress run\"",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@cypress/code-coverage": "^3.12.23",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.4",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "nyc": "^15.1.0",
    "playwright-webkit": "^1.41.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "start-server-and-test": "^2.0.3",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vite-plugin-istanbul": "^5.0.0"
  }
}
